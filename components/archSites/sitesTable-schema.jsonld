{
    "@context": {
        "csvw" : "http://www.w3.org/ns/csvw/",
        "dc" : "http://purl.org/dc/terms/",
        "schema" : "http://schema.org",
        "xsd" : "http://www.w3.org/2001/XMLSchema#",
        "poseidon" : "https://w3id.org/poseidon/v1.0/terms.rdf.jsonld#"
    },
    "@type" : "poseidon:SitesTableInfo",
    "csvw:columns" : [{
        "csvw:name" : "ID",
        "csvw:titles" : ["ID", "Site_ID"],
        "dc:description" : "The ID of the site. Can be any alphanumeric string including - and _",
        "csvw:datatype" : {
            "csvw:base" : {"@id" : "xsd:string"},
            "csvw:format" : "[a-zA-Z0-9_\\-]+"
        },
        "csvw:propertyUrl" : {"@id" : "dc:identifier"},
        "csvw:required" : true
    }, {
        "csvw:name" : "SiteName",
        "csvw:titles" : "SiteName",
        "dc:description" : "The name of the site",
        "csvw:datatype" : {"@id" : "xsd:string"},
        "csvw:propertyUrl" : {"@id" : "schema:name"},
        "csvw:required" : true
    }, {
        "csvw:name" : "CountryCode",
        "csvw:titles" : "CountryCode",
        "dc:description" : "Three-Letter ISO-Countrycode of the country that the site is in. See https://countrycode.org",
        "csvw:datatype" : {
            "csvw:base" : {"@id" : "xsd:string"},
            "csvw:format" : "[A-Z]{3}"
        },
        "csvw:propertyUrl" : {"@id" : "schema:addressCountry"},
        "csvw:required" : true
    }, {
        "csvw:name": "Province",
        "csvw:titles": "Province",
        "dc:description" : "The sub-national administrative region (e.g. Province) that the site is in",
        "csvw:datatype" : {"@id" : "xsd:string"},
        "csvw:propertyUrl" : {"@id" : "schema:name"}
    }, {
        "csvw:name": "Locality",
        "csvw:titles": "Locality",
        "dc:description" : "The nearest town or city of the site",
        "csvw:datatype" : {"@id" : "xsd:string"},
        "csvw:propertyUrl" : {"@id" : "schema:name"}
    }, {
        "csvw:name": "Latitude",
        "csvw:titles": "Latitude",
        "dc:description" : "A latitude in the WGS 84 standard coordinate system (see https://en.wikipedia.org/wiki/World_Geodetic_System), specified as decimal number of degrees.",
        "csvw:datatype" : {
            "csvw:base": "decimal",
            "csvw:minimum" : -90,
            "csvw:maximum": 90
        },
        "csvw:propertyUrl" : {"@id" : "schema:latitude"},
        "csvw:required" : true
    }, {
        "csvw:name": "Longitude",
        "csvw:titles": "Longitude",
        "dc:description" : "A longitude in the WGS 84 standard coordinate system (see https://en.wikipedia.org/wiki/World_Geodetic_System), specified as decimal number of degrees.",
        "csvw:datatype" : {
            "csvw:base": "decimal",
            "csvw:minimum" : -180,
            "csvw:maximum": 180
        },
        "csvw:propertyUrl" : {"@id" : "schema:longitude"},
        "csvw:required" : true
    }]
}